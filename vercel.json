{
  "version": 2,
  "builds": [
    {
      "src": "site/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/gallerij/?(.*)",
      "dest": "/site/gallerij/$1"
    },
    {
      "src": "/barcony-modellen/?(.*)",
      "dest": "/site/barcony-modellen/$1"
    },
    {
      "src": "/contact/?(.*)",
      "dest": "/site/contact/$1"
    },
    {
      "src": "/robots.txt",
      "dest": "/site/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/site/sitemap.xml"
    },
    {
      "src": "/media/(.*)",
      "dest": "/site/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/site/$1"
    }
  ],
  "headers": [
    {
      "source": "/media/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}
